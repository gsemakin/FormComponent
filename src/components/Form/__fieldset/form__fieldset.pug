mixin addFieldset(fieldset, langTmpl, div, addedClasses, selectedItems)
    - var id = fieldset[0]
    - var cl1 = addedClasses[id] ? addedClasses[id] : ""
    fieldset(id=id class=cl1)    
        ul.MMM--blockFormList(style="display:block")
            each item in fieldset[1]
                if typeof item === 'string'
                    include ./__field/_item.pug
                if Array.isArray(item)
                    include ./__field/_wrappedUl.pug
                           
+addFieldset(fieldset, langTmpl, div, addedClasses, selectedItems)